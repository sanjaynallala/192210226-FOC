#include <stdio.h>

int main()
{
    int n, sum = 0;

    // Input the value of 'n'
    printf("Enter the value of n: ");
    scanf("%d", &n);

    // Calculate the sum of squares of odd numbers up to 'n'
    for (int i = 1; i <= n; i += 2) {
        sum += i * i;
    }

    // Display the result
    printf("Sum of squares of odd numbers up to %d is: %d\n", n, sum);

    return 0;
}
